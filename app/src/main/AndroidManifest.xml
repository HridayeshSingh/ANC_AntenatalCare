<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.anc.antenatalcare.anc_antenatalcare">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HelpActivity"/>
        <activity android:name=".SelectPatientActivity" />
        <activity android:name=".DoctorEmailVerifyActivity" />
        <activity android:name=".TreatmentActivity"/>
        <activity android:name=".FeedbackActivity"/>
        <activity android:name=".LoginDoctorActivity"/>
        <activity android:name=".SignupDoctorActivity"/>
        <activity android:name=".DocPatDecisionActivity"/>
        <activity android:name=".UltrasoundActivity" />
        <activity android:name=".RoutineInvestigationsActivity"/>
        <activity android:name=".InvestigationsActivity"/>
        <activity android:name=".PregnancyDetailsActivity"/>
        <activity android:name=".ComplaintsActivity"/>
        <activity android:name=".ImageSamplesActivity"/>
        <activity android:name=".AppointmentDatesActivity"/>
        <activity android:name=".PatientDetails"/>
        <activity android:name=".LoginPatientActivity"/>
        <activity android:name=".SignupPatientActivity"/>
        <activity android:name=".MainPage"/>
        <activity android:name=".SignUpInDecisionActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".Service.MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".Service.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>